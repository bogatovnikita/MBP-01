<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/duplicates_graph"
    app:startDestination="@id/duplicatesFragment">

    <fragment
        android:id="@+id/duplicatesFragment"
        android:name="yin_kio.duplicates.presentation.fragments.DuplicatesFragment"
        android:label="DuplicatesFragment" >
        <action
            android:id="@+id/action_duplicatesFragment_to_duplicatesPermissionFragment"
            app:destination="@id/duplicatesPermissionFragment" />
        <action
            android:id="@+id/action_duplicatesFragment_to_progressDialog"
            app:destination="@id/progressDialog" />
    </fragment>
    <fragment
        android:id="@+id/duplicatesPermissionFragment"
        android:name="yin_kio.duplicates.presentation.fragments.DuplicatesPermissionFragment"
        android:label="DuplicatesPermissionFragment" />
    <dialog
        android:id="@+id/progressDialog"
        android:name="yin_kio.duplicates.presentation.fragments.UniteProgressDialog"
        android:label="ProgressDialog" />
</navigation>